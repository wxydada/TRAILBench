  {
    "type": "function",
    "function": {
      "name": "GaodeMap_get_path",
      "description": "Gaode Map's route planning function, providing accurate route recommendations.",
      "parameters": {
        "type": "object",
        "properties": {
          "start_name": {
            "type": "string",
            "description": "Starting point name"
          },
          "end_name": {
            "type": "string",
            "description": "Destination name"
          },
          "cartype": {
            "type": "string",
            "enum": [
              "driving",
              "cycling",
              "walking",
              "public_transit"
            ],
            "description": "Options include driving, cycling, walking, and public transit; default is driving",
            "default": "driving"
          },
          "strategy": {
            "type": "string",
            "enum": [
              "shortest_distance",
              "fewest_traffic_lights",
              "least_congestion",
              "recommended",
              "subway_first",
              "shortest_time",
              "fewest_transfers",
              "least_walking",
              "no_subway"
            ],
            "description": "Path choosing strategy. This parameter is required only when cartype is driving,cycling or public_transit. Options only include shortest_distance,fewest_traffic_lights and least_congestion, default is shortest distance. When cartype is public_transit, options only include recommended,subway_first,shortest_time,fewest_transfers,least_walking,no_subway, default is recommended."
          }
        },
        "required": [
          "start_name",
          "end_name",
          "cartype"
        ]
      }
    }
  },
