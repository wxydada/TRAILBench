  {
    "type": "function",
    "function": {
      "name": "Maoyan_search_show",
      "description": "Maoyan's show search function based on keywords, helping users discover concerts, theater performances, and live events, with a focus on entertainment booking.",
      "parameters": {
        "type": "object",
        "properties": {
          "keyword": {
            "type": "string",
            "description": "Search keywords (e.g., artist name,show name etc..)"
          },
          "show_type": {
            "type": "string",
            "enum": [
              "all",
              "concert",
              "drama_and_musical",
              "music_concert",
              "theater_play",
              "esports_tournament",
              "sports_event"
            ],
            "default": "all",
            "description": "Performance Type, used to filter shows; \"all\" indicates all content (i.e., no filtering)"
          },
          "start_time": {
            "type": "string",
            "description": "earliest show's time , format: %Y-%m-%d , e.g. 2024-05-28"
          },
          "end_time": {
            "type": "string",
            "description": "latest show's time , format: %Y-%m-%d , e.g. 2024-05-28"
          },
          "sort_by": {
            "type": "string",
            "enum": [
              "recommended_order",
              "best-selling",
              "distance_sorting"
            ],
            "default": "recommended_order",
            "description": "Sorting options"
          }
        },
        "required": [
          "keyword"
        ]
      }
    }
  },
