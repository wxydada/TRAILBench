  {
    "type": "function",
    "function": {
      "name": "Bilibili_search_video",
      "description": "Bilibili's video search function based on keywords, covering anime, creator content, and trending videos, ideal for youth-oriented interests.",
      "parameters": {
        "type": "object",
        "properties": {
          "keyword": {
            "type": "string",
            "description": "Search keywords (e.g., video title, creator, tags, etc.)"
          },
          "category": {
            "type": "string",
            "enum": [
              "comprehensive",
              "live",
              "movie",
              "anime",
              "tv_series"
            ],
            "description": "video categories"
          },
          "min_duration": {
            "type": "number",
            "description": "Minimum video duration (in minutes)"
          },
          "max_duration": {
            "type": "number",
            "description": "Maximum video duration (in minutes)"
          },
          "upload_date": {
            "type": "string",
            "enum": [
              "no_limit",
              "within_one_day",
              "within_one_week",
              "within_six_months"
            ],
            "description": "Upload date range",
            "default": "no_limit"
          },
          "sort_by": {
            "type": "string",
            "enum": [
              "latest",
              "popular",
              "high_views",
              "comprehensive"
            ],
            "description": "Sorting options",
            "default": "comprehensive"
          }
        },
        "required": [
          "keyword"
        ]
      }
    }
  },
