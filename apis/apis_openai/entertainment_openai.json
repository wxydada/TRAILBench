  {
    "type": "function",
    "function": {
      "name": "Maoyan_search_movie",
      "description": "Maoyan's movie schedule search based on keywords and location, helping users find showtimes and theaters for specific films.",
      "parameters": {
        "type": "object",
        "properties": {
          "keyword": {
            "type": "string",
            "description": "Search keywords (e.g., movie name.)"
          },
          "sort_by": {
            "type": "string",
            "enum": [
              "recommended_order",
              "distance_first",
              "lowest_price_first"
            ],
            "default": "recommended_order",
            "description": "Sorting options"
          },
          "start_time": {
            "type": "string",
            "description": "Earliest Movie Start Time , format: %H:%M, e.g. 6:30"
          },
          "end_time": {
            "type": "string",
            "description": "Latest Movie Start Time , format: %H:%M, e.g. 20:30"
          },
          "movie_version": {
            "type": "string",
            "enum": [
              "2d",
              "3d",
              "no_limit"
            ],
            "default": "no_limit",
            "description": "Movie Version"
          },
          "special_effects_theater": {
            "type": "string",
            "enum": [
              "imax",
              "dolby_atmos",
              "giant_screen",
              "laser_projection",
              "no_limit"
            ],
            "default": "no_limit",
            "description": "different special effects of theater"
          }
        },
        "required": [
          "keyword"
        ]
      }
    }
  },
